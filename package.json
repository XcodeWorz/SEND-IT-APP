  "bugs": {
    "url": "https://github.com/ozone4real/SEND-IT-APP/issues"
  },
  "engines": {
    "node": "10.x"
  },
  "homepage": "https://github.com/ozone4real/SEND-IT-APP#readme",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.2",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "config": "^2.0.1",
    "express": "^4.16.4",
    "jasmine": "^3.3.0",
    "jasmine-node": "^1.16.2",
    "jsonwebtoken": "^8.4.0",
    "mocha": "^5.2.0",
